    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-ts": "^5.5.1",
    "grunt-tslint": "^3.2.1",
    "mocha": "^3.0.2",
    "tslint": "^3.15.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
